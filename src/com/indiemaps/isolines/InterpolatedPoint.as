package com.indiemaps.isolines {		import flash.geom.Point;		public class InterpolatedPoint extends Point {				public var z:Number; // interpolated attribute value of this point		public var eID:int; // the key position of the edge		public var lineThrough:Boolean = false; // stores whether or not the point has a line through it				public function InterpolatedPoint(_x,_y,_z,_eID=null) {						this.x = _x;			this.y = _y;			this.z = _z;		}	}}